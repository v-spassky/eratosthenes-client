{
    "plugins": [
        "react",
        "simple-import-sort"
    ],
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "rules": {
        "quotes": ["error", "double"],
        "simple-import-sort/imports": "warn",
        "simple-import-sort/exports": "warn",
        "react/react-in-jsx-scope": "off",
        "react/prop-types": "off",
        "no-unused-vars": ["warn", { "varsIgnorePattern": "^_", "argsIgnorePattern": "^_" }]
    },
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": "latest"
    },
    "env": {
        "browser": true,
        "node": true
    }
}